  "setId": "ST07",
  "cardNumber": "008",
  "printedText": "[On Play] Look at up to 1 Life card from the top of your or your opponent's Life Area and put it either on the top or the bottom of the Life Area.",
  "keywords": [],
  "abilities": [
    {
      "timing": "onPlay",
      "selectors": {
        "selfLifeCards": {
          "side": "self",
          "type": "any",
          "zones": ["life"]
        },
        "opponentLifeCards": {
          "side": "opponent",
          "type": "any",
          "zones": ["life"]
        }
      },
      "actions": [
        {
          "type": "chooseMode",
          "min": 0,
          "max": 1,
          "whoChooses": "self",
          "modes": [
            {
              "actions": [
                {
                  "type": "revealLife",
                  "count": 1,
                  "side": "self"
                },
                {
                  "type": "moveCard",
                  "target": "selfTopLifeCard",
                  "destination": "topOrBottomOfLife",
                  "ordering": "keep"
                }
              ]
            },
            {
              "actions": [
                {
                  "type": "revealLife",
                  "count": 1,
                  "side": "opponent"
                },
                {
                  "type": "moveCard",
                  "target": "opponentTopLifeCard",
                  "destination": "topOrBottomOfLife",
                  "ordering": "keep"
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}
